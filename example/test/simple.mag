import std
import std.io

data A = U | V ;

data T1 = C1(T2) ;
data T2 = C2(T1) ;

data Nat = O | S(Nat);

f: (n: i32) -> i32 {
	let x: i32 = 2 + 3 + 4 + 5;
    x
}

swap: (@x: A, @y: A) -> () {
	let tmp : A = x;
	x = y;
	y = tmp;
}

g: () -> str {
	"hello, world"
}

add: (n: Nat, m: Nat) -> Nat {
	match n {
		O => m,
		S(k) => S(add(k, m))
	}
}

prim: (n : Nat) -> i32 {
	match n {
		O => 0,
		S(k) => prim(k) + 1
	}
}

main: () -> () {
	let x : A = U;
	let y : A = V;
	let w : i32 = 10;
	let z : i32 = f(w);
	let n : Nat = S(S(O));
	swap(@x, @y);
	std.io.println<i32>(w);
	io.println<i32>(z);
}
